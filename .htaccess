
<IfModule mod_rewrite.c>
RewriteEngine On

# RewriteCond %{HTTP:X-Forwarded-Protocol} !=https
# RewriteRule .* https://%{SERVER_NAME}%{REQUEST_URI} [R=301,L]

RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
